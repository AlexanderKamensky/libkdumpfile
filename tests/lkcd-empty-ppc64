#! /bin/sh

#
# lkcd-empty-ppc64
#
# Create an empty LKCDv9 dump file and verify meta-attributes.
#

arch=ppc64
byte_order=0
ptr_size=8
page_shift=16
page_size=65536
page_offset=0xC000000000000000

panic="Test panic string"
node=test-node
sysname=Linux
release=3.4.5-test
version="#1 SMP Fri Jan 22 14:02:42 UTC 2016 (1234567)"
machine=ppc64
domain="(none)"

. "$srcdir"/lkcd-empty
